#!/bin/bash

## This is for the development environment

R1=$(realpath "$BASH_SOURCE" ) ## realpath needed to resolve symlinks; R1 will be the path to this file
R2=$(dirname "$R1") # path containing this file, namely the installation folder
R=$( cd "$R2"  && pwd) ## absolute path, still might have spaces

# mac-setup is needed to fix Mac security complaints, though it is only
# needed once
if [ "$( uname )" = "Darwin" ] ; then
  "${ROOT}/mac-setup" > /tmp/openjmlsetup 2>&1 ;
fi

BIN=`echo $R/jdk/bin/java`

$BIN "$@"
